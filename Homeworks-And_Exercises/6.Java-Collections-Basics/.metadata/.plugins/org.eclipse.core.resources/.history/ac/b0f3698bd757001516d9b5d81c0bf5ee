import java.util.Arrays;
import java.util.Scanner;



public class SortArrayOfNumbers {

	public static void main(String[] args) {
		System.out.println("Please enter the number of integers you want to be sorted:");
		
		Scanner inputNumbers = new Scanner(System.in);
		int numberOfElements = Integer.parseInt(inputNumbers.nextLine());
		
		System.out.printf("Please, enter %d integers:\n", numberOfElements);
		int[] numbers = new int[numberOfElements];
		for (int i = 0; i < numbers.length; i++) {
			numbers[i] = inputNumbers.nextInt();
		}
		
		Arrays.sort(numbers);
		
		System.out.println(String.join(" ", numbers.toString()));
	}

}
