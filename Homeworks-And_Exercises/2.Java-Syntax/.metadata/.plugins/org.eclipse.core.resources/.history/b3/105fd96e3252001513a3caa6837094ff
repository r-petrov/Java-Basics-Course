import java.util.Scanner;


public class Test {
	public static void main(String[] args) {
		int[] numbers = {0,1,2};
		for (int i : numbers) {
			System.out.println(i);
		}
		changeArray(numbers);
		
		for (int i : numbers) {
			System.out.println(i);
		}
	}
	
	static int[] changeArray (int[] numbers){
		for (int i = 0; i < numbers.length; i++) {
			numbers[i] += 1; 
		}
		return numbers;
	}
}
