package Display;

import java.awt.Frame;

import javax.swing.JFrame;

public class Display {
	private String title;
	private int width;
	private int height;
	
	private JFrame frame;
	
	public Display(String title, int width, int height) {
		super();
		this.title = title;
		this.width = width;
		this.height = height;
	}
	
	private void createFrame() {
		this.frame = new JFrame();
	}
}
